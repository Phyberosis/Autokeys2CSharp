<Application x:Class="Autokeys2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Autokeys2"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/MainResources.xaml"/>
                <ResourceDictionary Source="/Resources/MainStyles.xaml"/>
                <ResourceDictionary Source="/Resources/MacrosTreeStyle.xaml"/>
                <ResourceDictionary Source="/Resources/MacrosViewStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
