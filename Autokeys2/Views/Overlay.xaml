<Window x:Class="Autokeys2.Views.Overlay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Autokeys2.Views"
        mc:Ignorable="d" WindowState="Minimized" ResizeMode="NoResize" ShowInTaskbar="False"
        WindowStyle="None" Background="Transparent" AllowsTransparency="True" Loaded="Window_Loaded">
    
    <Border Name="border" BorderThickness="3" CornerRadius="5" Background="Transparent">
        <Border.BorderBrush>
            <SolidColorBrush Color="{StaticResource blue0}"/>
        </Border.BorderBrush>
        <Canvas Name="canvas" Background="Transparent">
        </Canvas>
    </Border>
</Window>
